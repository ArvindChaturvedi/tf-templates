{
  "application_name": "team-b-app",
  "database_name": "team-b-db",
  "master_username": "team-b-admin",
  "instance_count": 1,
  "instance_class": "db.r5.xlarge",
  "allowed_cidrs": ["10.1.0.0/16"],
  "backup_retention_period": 14,
  "performance_insights_enabled": true,
  "performance_insights_retention_period": 14,
  "monitoring_interval": 30,
  "db_cluster_parameters": {
    "log_statement": "mod",
    "log_min_duration_statement": "500"
  },
  "db_instance_parameters": {
    "work_mem": "32MB",
    "maintenance_work_mem": "2GB"
  },
  "create_cloudwatch_alarms": true,
  "create_sns_topic": true,
  "create_db_credentials_secret": true,
  "generate_master_password": true,
  "create_db_event_subscription": true,
  "create_db_access_role": true,
  "create_eks_integration": true,
  "eks_cluster_name": "team-b-cluster",
  "eks_namespace": "team-b",
  "create_pgbouncer": true,
  "pgbouncer_instance_type": "t3.small",
  "pgbouncer_min_capacity": 2,
  "pgbouncer_max_capacity": 4,
  "pgbouncer_desired_capacity": 3
} 